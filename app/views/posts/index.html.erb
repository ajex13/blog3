<h1 id="page_title">Stuff I've written</h1>


<div id="posts_wrapper" class="skinny_wrapper">

  <div class="post">
      <%  @posts.each do |post| %>
    <p class="date"><%= post.created_at.strftime("%b %e, %l:%M %p")%></p>
      <h2><%= link_to post.title, post %> </h2>
      <hr>
      <% end %>
  </div>

</div>

<%= will_paginate @posts %>
